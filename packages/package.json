    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^12.1.5",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@wordpress/eslint-plugin": "^13.7.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.9.4",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@elementor/ui": "^1.2.1"
